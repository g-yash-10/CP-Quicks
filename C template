#include <stdio.h>
int max(int a, int b)
{
  if(a>b)
    return a;
  return b;
}
int min(int a, int b)
{
  if(a<b)
    return a;
  return b;
}

int main(){
  int t;
  scanf("%d", &t);
  while(t-->0){
    int n;
    scanf("%d", &n);
    int arr[n];
    int i;
    for(i=0;i<n;i++)
        scanf("%d", &arr[i]);
  }
  return 0;
}
